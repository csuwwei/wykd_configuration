package com.wykd.user.dao;

import java.util.List;

import org.apache.ibatis.annotations.Insert;
import org.apache.ibatis.annotations.Mapper;
import org.apache.ibatis.annotations.Param;
import org.apache.ibatis.annotations.Select;

import com.wykd.user.bean.UserInfo;

@Mapper
public interface IUserDao {

	@Insert("insert into sec_user(userName,age) values(#{userName},#{age})")
	int add(@Param("userName")String userName,@Param("age")int sex);
	
	@Select("select * from sec_user")
	List<UserInfo> getUserList();
}
